<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="jp.osdn.dddsupport.quickclassgenerator.quickClassGenerateCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="jp.osdn.dddsupport.quickclassgenerator.internal.action.QuickClassGenerateCommandHandler"
            commandId="jp.osdn.dddsupport.quickclassgenerator.quickClassGenerateCommand">
         <enabledWhen>
            <and>
               <count
                     value="1">
               </count>
               <iterate
                     ifEmpty="false">
                  <instanceof
                        value="org.eclipse.jdt.core.IPackageFragment">
                  </instanceof>
               </iterate>
            </and>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="jp.osdn.dddsupport.quickclassgenerator.quickClassGenerateCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+F9">
      </key>
   </extension>

</plugin>
