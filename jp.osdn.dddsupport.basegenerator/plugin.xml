<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="jp.osdn.dddsupport.basegenerator.baseClassCreateCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?after=additions">
         <menu
               label="%menu.label">
            <command
                  commandId="jp.osdn.dddsupport.basegenerator.baseClassCreateCommand"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <and>
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <adapt
                              type="org.eclipse.jdt.core.ICompilationUnit">
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:#CompilationUnitEditorContext?after=additions">
         <menu
               label="%menu.label.0">
            <command
                  commandId="jp.osdn.dddsupport.basegenerator.baseClassCreateCommand"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="jp.osdn.dddsupport.basegenerator.internal.action.BaseClassGenerateCommandHandler"
            commandId="jp.osdn.dddsupport.basegenerator.baseClassCreateCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="jp.osdn.dddsupport.basegenerator.baseClassCreateCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+F8">
      </key>
   </extension>

</plugin>
